            <string>/// AI


accel = 0

// start mode
if mode == 0 {    // Idle
    if timeout &lt;= 0 {
        phase = irandom(1)
        image_blend = c_white
        switch phase {    // start phase
        case 0: {       // stay
            timeout = random(60)+30
            forward = irandom(1)*2 - 1
            sprite_index = sp_girl_stay
            image_speed = 0.1
//            hspeed = 0
            break
        }  
        case 1: {       // walk
            timeout = random(20)+10
            forward = irandom(1)*2 - 1
            sprite_index = sp_girl_walk
            image_speed = 0.1
            break    
        }
        }
    } 
    timeout--
}


// process modes
if mode == 0 {   // idle
   // process phase
    if phase == 1 {
        accel = 0.1
        if feetcollision {
            sprite_index = sp_girl_walk
            if sidecollision {
               vspeed = -3
            }
        } 
        if feetcollision == false {
            sprite_index = sp_girl_jump
            if vspeed &gt; 0 image_index = 1
            if vspeed &lt; 0 image_index = 0
            accel = 0
        }
    }    
}

if mode == 1 {   // attack
    accel = 0.15
    if feetcollision {
        sprite_index = sp_girl_walk
        if sidecollision {
           vspeed = -3
        }
    } else {
        sprite_index = sp_girl_jump
        if vspeed &gt; 0 image_index = 1
        if vspeed &lt; 0 image_index = 0
        accel = 0
    }
    
}

// ALWAYS
if feetcollision {
  hspeed *= 0.88
  gravity = 0
} else 
  gravity = 0.2  


hspeed+=accel*forward

if abs(hspeed) &gt; maxspeed hspeed = maxspeed*forward
if vspeed&gt;fallmaxspeed    vspeed = fallmaxspeed
  
// check range ahead  
var _x, _y, i=0, j=0, ok = false;
//repeat (2) {
//    j++
//    _y = y - 16*j
//    i=0
    repeat (range) {
        i++
        _x = x + 15*i*forward
        _y = y - 16
        if collision_point(_x, _y, hero, false, true) {
            mode = 1     // start attack
            maxspeed = 3
            sprite_index = sp_girl_walk
            ok = true
            break
        }
//        instance_create(_x, _y, ob_mark)
        if collision_point(_x, _y, ob_wall, false, true) {
            break
        }  
    }   
//    if ok break
//}

if not ok {
    mode = 0
}    
    



image_xscale = forward
feetcollision = false
sidecollision = false

</string>
